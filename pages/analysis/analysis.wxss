/* 表格代码   */
.table{
  border:1px  solid #dadada;
  border-radius: 20px;
  border-right:0;
  border-bottom: 0;
  width: 660rpx;
  margin-left: 1%;
 height: 500rpx;
}
.tr{
  width:100%;
  display: flex;
  justify-content: space-between;

}
.th,.td{
  padding: 10px;
  background-color: white;
  border-bottom: 1px solid #dadada;
  text-align: center;
  width: 100%;
}
.th1,.th2,.td1,.td2{
  width: 100%;
  height: 60rpx;
}

.th.th3{
  border-radius: 0 20px 0 0;
}
.th{
  border-radius: 20px 0 0 0;
  font-weight: 500;
  background-color: #f0f0f0;
  font-size: 12px;
  color: #422fcd;
  border-right: 1px solid #dadada;
}
.td{
  display: flex;
  justify-content: center;
  font-size: 20rpx;
  color: black;
 
}
.td.td33{
  border-radius: 0 0 20px 0;
}
.td.td11{
  border-radius: 0 0 0 20px;
}
